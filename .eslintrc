{
  "env": {
    "browser": true,
    "es6": true
  },

  "extends": "eslint:recommended",

  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },

  "rules": {
    "semi": ["error", "always"],
    "quotes": ["error", "single"],
    "no-multiple-empty-lines": "error",
    "indent": ["error", 2]
  }
}


